{
  "name": "@sunshj/mdc",
  "version": "0.1.3",
  "description": "Prose components for Nuxt MDC.",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "require": "./dist/module.cjs",
      "import": "./dist/module.mjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.11",
    "@iconify-json/vscode-icons": "^1.2.2",
    "@nuxt/icon": "^1.6.1",
    "@nuxt/image": "^1.8.1",
    "@nuxt/kit": "^3.13.2",
    "@nuxtjs/mdc": "^0.9.2",
    "@vueuse/core": "^11.2.0",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@sunshj/eslint-config": "^2.1.2",
    "@sunshj/prettier-config": "^1.0.2",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.10.0",
    "lint-staged": "15",
    "nuxt": "^3.13.2",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "typescript": "latest",
    "vue-tsc": "^2.1.10"
  },
  "pnpm": {
    "overrides": {
      "nitropack": "npm:nitropack@2.9.7"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,json,md,yaml}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@sunshj/prettier-config",
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
